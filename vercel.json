{
  "version": 2,
  "builds": [
    {
      "src": "pirateone_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "pirateone_backend/wsgi.py"
    }
  ],
  "functions": {
    "pirateone_backend/wsgi.py": {
      "runtime": "python3.11"
    }
  }
}